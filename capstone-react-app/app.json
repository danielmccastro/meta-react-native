{
  "expo": {
    "name": "capstone-react-app",
    "slug": "capstone-react-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": {
      "Karla": {
        "regular": "./assets/fonts/Karla-Regular.ttf"
      },
      "Markazi": {
        "regurlar": "./assets/fonts/MarkaziText-Regular.ttf"
      }
    }
  }
}
